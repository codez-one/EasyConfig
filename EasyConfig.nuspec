<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>easyconfig</id>
    <title>Easy Config</title>
    <version>1.0.0</version>
    <authors>Kirsten Kluge</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/codez-one/EasyConfigSiteExtension</projectUrl>
    <repository type="git" url="https://github.com/codez-one/EasyConfigSiteExtension.git" branch="master" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
