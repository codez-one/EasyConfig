<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>EasyConfig.SiteExtension</id>
    <title>Easy Config</title>
    <version>$version$</version>
    <authors>Kirsten Kluge</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/codez-one/EasyConfig</projectUrl>
    <repository type="git" url="https://github.com/codez-one/EasyConfig.git" branch="master" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An endpoint for SPA client apps hosted in an azure web app to get configuration values stored in environment variables.</description>
    <icon>images/icon.png</icon>
    <tags>AzureSiteExtension SPA</tags>
    <packageTypes>
      <packageType name="AzureSiteExtension" />
    </packageTypes>
  </metadata>
  <files>
    <file src="content/**/*.*" target="/content" />
    <file src="Images/icon.png" target="/images" />
  </files>
</package>